# Launch terminal
super + Return
  "$TERMINAL" -e "$INTERACTIVE_SHELL"

# Rofi run
control + space
  rofi -show run

# Rofi drun
super + d
  rofi -show drun

# Rofi switch window
super + space
  rofi -show window

# Rofi pass
super + p
  rofi-pass

# Rofi buku_run
super + b
  buku_run

# Take screenshot
Print
  flameshot gui

# Greenclip clipboard manager
super + c
  rofi -modi "clipboard:greenclip print" -show clipboard -run-command '{cmd}'
