# Launch terminal
super + Return
  "$TERMINAL" -e "$INTERACTIVE_SHELL"

# Launch browser
super + shift + f
  $BROWSER

# Rofi run
alt + space
  rofi -show run

# Rofi drun
super + d
  rofi -show drun

# Rofi switch window
super + space
  rofi -show window

# Password manager
super + p
  bwmenu

# Rofi buku_run
super + b
  buku_run

# Take screenshot
Print
  flameshot gui

# Greenclip clipboard manager
super + c
  rofi -modi "clipboard:greenclip print" -show clipboard -run-command '{cmd}'
